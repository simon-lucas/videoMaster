package StateMachine{public class GenericState extends Object{	public var previous:String;	public var debug:Boolean=false;	public var locked:Boolean=false;	public var actor:*;	public var stateName:String = "name"	public function GenericState(actor:*)	{		super();		this.actor=actor;	}		public function unLock():void{		if (debug) trace(this, "unlocked")		locked=false;	}	}}