package StateMachine{import VideoApp;import data.Global;public class VideoState extends GenericState implements IState	{	public function VideoState(actor:*)	{		super(actor);	}		public function enter():void	{		debug=true;		Global.videoPlaying = true		actor.showStage(VideoApp.VIDEO)		if (debug) trace("start ", VideoApp.VIDEO, "state")	}		public function update(tickCount:uint):void	{	}		public function exit():void	{		actor.videoSprite.deInit()		actor.hideStage(VideoApp.VIDEO)		Global.videoPlaying = false		if (debug) trace("exit ", VideoApp.VIDEO, "state")	}	}}