package VideoMaster {		import flash.display.MovieClip;	import views.StageVideoPlayer;	import com.spiral.events.VideoEventZ;	import flash.events.Event;	import flash.events.MouseEvent;			public class Intro extends MovieClip {				public var vid: StageVideoPlayer;		public function Intro() {			// constructor code		}				public function init(vidName:String):void{			vid= new StageVideoPlayer()			vid.addEventListener(VideoEventZ.VIDEO_STOP, videoEnded)			this.bar.width=1			vid.bar = this.bar			vid.barBg = this.barBG			this.bg.visible=false				addChild(vid)			vid.init(vidName)			addEventListener(MouseEvent.MOUSE_DOWN, videoEnded)		}				public function deInit():void{			trace("Intro:deInit")			vid.stopVideo()			vid.removeEventListener(VideoEventZ.VIDEO_STOP, videoEnded)			vid = null;		}				public function videoEnded(e:Event):void{			dispatchEvent(new Event("introEnded"));		}			}	}