package VideoMaster {		import flash.display.MovieClip;	import views.StageVideoPlayer;	import com.spiral.events.VideoEventZ;	import flash.events.Event;	import flash.events.MouseEvent;			public class Intro extends MovieClip {				public var vid: StageVideoPlayer;		public function Intro() {			// constructor code		}				public function init(vidName:String):void{			trace("vid init")			vid= new StageVideoPlayer()			vid.addEventListener(VideoEventZ.VIDEO_STOP, videoEnded)			this.bar.width=1			vid.bar = this.bar			vid.barBg = this.barBG			this.bg.visible=false				addChild(vid)			vid.init(vidName)			addEventListener(MouseEvent.MOUSE_DOWN, videoEnded)						var back = new SkipVideoSprite()			//back.initProps(12,"regular",280)						addChild(back)			back.x= stage.stageWidth - (back.width+50)			back.y= 50					}				public function deInit():void{			trace("Intro:deInit")			vid.stopVideo()			vid.removeEventListener(VideoEventZ.VIDEO_STOP, videoEnded)			vid = null;		}				public function videoEnded(e:Event):void{			dispatchEvent(new Event("introEnded"));		}			}	}