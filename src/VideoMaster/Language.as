package VideoMaster {		import flash.display.MovieClip;	import flash.utils.setTimeout;	import flash.events.MouseEvent;	import data.Global;	import flash.events.Event;			public class Language extends MovieClip {						public function Language() {			// constructor code		}				public function init():void{			trace("init lang")			lang1.addEventListener(MouseEvent.MOUSE_DOWN, selectLang)			lang2.addEventListener(MouseEvent.MOUSE_DOWN, selectLang)		}				public function selectLang(e:MouseEvent):void{			if (e.currentTarget.name=="LangButton1"){				Global.lang="A"			}else{				Global.lang="E"			}			setTimeout(goNext,500)		}				public function goNext():void{			dispatchEvent(new Event("goMenu"));		}				public function deInit():void{			trace("lang deInit")					}			}	}