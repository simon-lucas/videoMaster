package VideoMaster {		import flash.display.MovieClip;	import flash.events.MouseEvent;	import flash.events.Event;	import data.Global;			public class Menu extends MovieClip {				public const MENUCOUNT : uint = 4					public function Menu() {			// constructor code		}				public function init() {			// constructor code			for (var i:int = 0; i < MENUCOUNT; i++)			{				this["menu"+(i+1)].visible = true;				this["menu"+(i+1)].menuIndex = i;				this["menu"+(i+1)].addEventListener("onMenuPress", onMenuPress);			}		}				public function onMenuPress(e:Event):void{			trace(e.currentTarget.menuIndex)			Global.currentVideoIndex = e.currentTarget.menuIndex				dispatchEvent(new Event("playVideoStory"))		}	}	}