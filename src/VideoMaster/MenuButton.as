package VideoMaster {		import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;			public class MenuButton extends MovieClip {				public var menuIndex:uint;				public function MenuButton() {			// constructor code			trace("MenuButton")			addEventListener(MouseEvent.MOUSE_DOWN, onPressButton)		}				public function onPressButton(e:MouseEvent):void{			dispatchEvent(new Event("onMenuPress"))		}	}	}