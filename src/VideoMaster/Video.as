package VideoMaster {		import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;	import com.spiral.events.VideoEventZ;	import views.StageVideoPlayer;			public class Video extends MovieClip {				private var vid: StageVideoPlayer;				public function Video() {			// constructor code		}				public function init(vidName:String):void{			//trace("init V", this)			vid= new StageVideoPlayer()			vid.addEventListener(VideoEventZ.VIDEO_STOP, videoEnded)			this.bar.width=1			vid.bar = this.bar			vid.barBg = this.barBG			this.bg.visible=false				this.addChild(vid)							vid.init(vidName)			addEventListener(MouseEvent.MOUSE_DOWN, videoEnded)		}				public function deInit():void{			trace("video deInit")			vid.stopVideo()			vid.removeEventListener(VideoEventZ.VIDEO_STOP, videoEnded)			vid = null;		}				public function videoEnded(e:Event):void{			dispatchEvent(new Event("videoEnded"));		}	}	}