package com.studioshitless.GraphicTimers{import com.studioshitless.GraphicTimers.PlainPie;import com.greensock.TweenMax;import NMSEnergyBalance.Global;import com.greensock.easing.Elastic;import com.greensock.easing.Cubic;public class PieChart extends PlainPie{	public var currentDegrees:Number=0;	public var ANIMSPEED:uint;	public function PieChart(animationSpeed:uint)	{		super();		ANIMSPEED=animationSpeed 	}				public function updateDegrees(newDegrees:int):void{		TweenMax.to(this, ANIMSPEED/2.0, {currentDegrees:newDegrees, ease:Cubic.easeInOut, onUpdate:setUpdatedValue});	}		public function setUpdatedValue():void{		//trace("currentDegrees", currentDegrees)		this.setDegrees(currentDegrees)	}}}