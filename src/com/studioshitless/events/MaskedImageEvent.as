package com.studioshitless.events{import flash.events.Event;import com.spiral.maryrose.ContextGallery.data.MRObject;import flash.geom.Point;public class MaskedImageEvent extends Event{		public static const LOADED:String = "onLoaded";	public static const ZOOM:String = "onZoom";	public static const FINISH_ZOOM:String = "onFinishZoom";		public  var zoomed:Boolean;	public  var mrObjectRef:MRObject;	public var globalTopLeft:Point;	public function MaskedImageEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)	{		super(type, bubbles, cancelable);	}		override public function clone():Event	{		return new MaskedImageEvent(type, bubbles, cancelable);	}	}}