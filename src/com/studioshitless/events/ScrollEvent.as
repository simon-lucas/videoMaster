package com.studioshitless.events{import flash.events.Event;public class ScrollEvent extends Event{		public static const ON_START:String = "onStartPress";	public static const ON_STOP:String = "onStopPress";		public var direction:String;	public function ScrollEvent(type:String, bubbles:Boolean=true, cancelable:Boolean=false)	{		super(type, bubbles, cancelable);	}		override public function clone():Event	{		return new ScrollEvent(type, bubbles, cancelable);	}	}}